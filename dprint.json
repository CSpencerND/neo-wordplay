{
    "lineWidth": 96,
    "indentWidth": 4,
    "quoteStyle": "preferDouble",
    "quoteProps": "preserve",
    "semiColons": "asi",
    "typescript": {},
    "json": {},
    "includes": [
        "**/*.{ts,tsx,js,jsx,cjs,mjs,json}"
    ],
    "excludes": [
        "**/node_modules",
        "**/*-lock.json"
    ],
    "plugins": [
        "https://plugins.dprint.dev/typescript-0.83.0.wasm",
        "https://plugins.dprint.dev/json-0.17.0.wasm"
    ]
}
